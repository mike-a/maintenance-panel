{
    "name": "vivinet/maintenance-panel",
    "description": "A maintenance panel to easily manage core packages",
    "type": "project",
    "license": "All rights MikeA, Edinburgh, Scotland",
    "authors": [
        {
            "name": "Mike A.",
            "email": "mike.a@hotmail.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mike-a/user-management.git"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0.1",
        "vivinet/user-management": "master"
    },
    "require-dev": {
        "orchestra/testbench": "^5.17.0"
    },
    "autoload": {
        "psr-4": {
            "Vivinet\\MaintenancePanel\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Vivinet\\MaintenancePanel\\Tests\\": "tests"
        }
    }
